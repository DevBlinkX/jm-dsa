{%extends 'web/rm-base-dashboard.html.j2'%}
{% block title %} DRA Signup | Blinkx{% endblock %}
{%block custom_css%}
<style>
    .heading {
        font-size: 18px;
        font-weight: 500;
        margin-bottom: 10px;
        color: black;
    }

    #signup-dra-modal .modal-dialog {
        max-width: 600px;
    }
a:hover{
text-decoration: none !important;    
}
    #signup-dra-modal .modal-body {
        overflow: hidden;
        border-radius: 16px;
        padding: 34px 34px !important;
    }

    #signup-dra-modal .close {
        top: -10px;
        right: -10px;
    }

    .text-black {
        color: black;
    }

    textarea::placeholder {
        font-size: 14px;
        color: #858796;
    }

    .modal .close {
        z-index: 1;
        font-size: 2rem;
        font-weight: 400;
        border-radius: 50%;
        padding: 3px 9px;
        color: #ca8e8e;
        opacity: 1;
        background: #efddd4;
    }
</style>
{%endblock%}
{% block content %}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jquery-nice-select/1.1.0/css/nice-select.min.css"
    integrity="sha512-CruCP+TD3yXzlvvijET8wV5WxxEh5H8P4cmz0RFbKK6FlZ2sYl3AEsKlLPHbniXKSrDdFewhbmBK5skbdsASbQ=="
    crossorigin="anonymous" referrerpolicy="no-referrer" />
<!-- Content Wrapper -->
<div id="content-wrapper" class="d-flex flex-column">

    <!-- Main Content -->
    <div id="content">

        <!-- Topbar -->
        <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top py-5 justify-content-between">

            <!-- Sidebar Toggle (Topbar) -->
<!-- deleted -->

            <!-- Topbar Search -->
            <div class="top-heading">
                <p class="h3 font-weight-600">DRA Signup</p>

            </div>
            {%include 'web/dashboard/_partials/rm/header.html.j2'%}

        </nav>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container-fluid">

            <div class="row">
                <div class="col-12 col-xl-12 col-lg-12">
                    <div class="d-flex justify-content-end mb-3">
                        <select class="w-auto custom-select days-select" name="" id="">
                            <option selected disabled
                                data-display="<i class='fa fa-filter' aria-hidden='true'></i> Filter">Filter</option>
                            <option value="30">Last 30 days</option>
                            <option value="60">Last 60 days</option>
                            <option value="90">Last 90 days</option>
                        </select>
                    </div>
                    <!-- DataTales Example -->
                    <div class="card border-0 mb-4">

                        <div class="card-body p-0">
                            <div class="table-responsive">
                                <table class="table" id="dataTable" width="100%" cellspacing="0">
                                    <thead class="bg-beige">
                                        <tr class="">
                                            <th>NAME</th>
                                            <th>EMAIL</th>
                                            <th>MOBILE N0</th>
                                            <th>STAGE</th>
                                            <th>CREATED</th>
                                            <th>AGREEMENT</th>
                                            <th>ADMIN STATUS</th>
                                        </tr>
                                    </thead>
                                    <tbody data-rm-signup>

                                    </tbody>
                                </table>
                                <div data-empty-category style="display: none;" class="card w-100 mt-4">
                                    <p class="h6 text-center py-5">No Data Available</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <p id="pagination-here" class="d-flex justify-content-center"></p>

        </div>
        <!-- /.container-fluid -->

    </div>
    <!-- End of Main Content -->

    <!-- Footer -->
    <!-- <footer class="sticky-footer bg-white">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright &copy; Your Website 2021</span>
                    </div>
                </div>
            </footer> -->
    <!-- End of Footer -->

</div>
<!-- End of Content Wrapper -->



<!-- Logout Modal-->
<div class="modal fade" id="signup-dra-modal" tabindex="-1" role="dialog" aria-labelledby="signup-dra-modal"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">

            <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">Ã—</span>
            </button>

            <div class="modal-body p-0">
                <div class="text-black font-weight-600 px-3 heading mb-4">DRA Incentive</div>
                <form action="/api/rm/dra/signup/" method="post" id="rm-modal-form" data-rm-form>

                    <input type="text" hidden name="username" id="username-form" class="form-control"
                        aria-describedby="usernameHelp">
                    <div class="col-12 form-group ">
                        <input type="number" class="form-control" id="incentive" aria-describedby="incentiveHelp"
                            required name="incentive_per_lead">
                        <label class="form-control-placeholder" for="incentive">Per User Incentive*</label>
                        <!-- <div id="incentiveHelp" class="form-text text-danger">Per User Incentive</div> -->
                    </div>
                    <div class="col-12 form-group ">
                        <input class="form-control" id="brokerage" aria-describedby="brokerageHelp" required
                            name="brokerage_percentage" type="number" max="30">
                        <label class="form-control-placeholder" for="brokerage">Brokerage share*</label>
                        <!-- <div id="brokerageHelp" class="form-text text-danger">Brokerage share</div> -->
                    </div>
                    <div class="col-12 form-group ">
                        <textarea class="form-control" id="tnc" name="term_condition" placeholder="Terms and Conditions"
                            rows="4"></textarea>
                    </div>
                    <button type="submit" class="btn gradient-btn">Submit</button>
                </form>
            </div>

        </div>
    </div>
</div>
{% endblock %}
