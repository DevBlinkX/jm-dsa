                     <div class="d-flex justify-content-start position-relative otp-form otp-form-footer">
                        <input class="otp  otp-footer otp-1-footer" type="text" oninput='digitValidateFooter(this)' onkeyup='tabChangeFooter(1)' maxlength=1
                        required>
                        <input class="otp  otp-footer otp-2-footer" type="text" oninput='digitValidateFooter(this)' onkeyup='tabChangeFooter(2)' maxlength=1
                        required>
                        <input class="otp  otp-footer otp-3-footer" type="text" oninput='digitValidateFooter(this)' onkeyup='tabChangeFooter(3)' maxlength=1
                        required>
                        <input class="otp  otp-footer otp-4-footer" type="text" oninput='digitValidateFooter(this)' onkeyup='tabChangeFooter(4)' maxlength=1
                        required>
                        <input class="otp  otp-footer otp-5-footer" type="text" oninput='digitValidateFooter(this)' onkeyup='tabChangeFooter(5)' maxlength=1
                        required>
                        <input class="otp  otp-footer otp-6-footer" type="text" oninput='digitValidateFooter(this)' onkeyup='tabChangeFooter(6)' maxlength=1
