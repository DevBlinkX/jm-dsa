    <script>
        $("#rm-logout-btn").on("click", function () {
            console.log("this is clicked");
            localStorage.removeItem('access-token');
            window.location.href = '/logout';
        });
    </script>
<script>
    var width = $(window).width();
    if (width <= 991) {
      $('.sidebar').addClass('toggled');
    } else {
        $('.sidebar').removeClass('toggled');
    }
</script>
  <script>
  document.addEventListener('keyup', (e) => {
    if (e.key == 'PrintScreen') {
        navigator.clipboard.writeText('');
        alert('Screenshots disabled!');
    }
  });

  /** TO DISABLE PRINTS WHIT CTRL+P **/
  document.addEventListener('keydown', (e) => {
      if (e.ctrlKey && e.key == 'p') {
          alert('This section is not allowed to print or export to PDF');
          e.cancelBubble = true;
          e.preventDefault();
          e.stopImmediatePropagation();
      }
  });

     $(function() {
            $(this).bind("contextmenu", function(e) {
                e.preventDefault();
            });
        });

  </script>
    <script>
        $('#sidebarToggleTop').click(function(){
            $('.mobile_overlay').addClass('mobile_overlay_click');
            $('#content-wrapper').addClass('screen-lock');
           
        })
        $('.mobile_overlay').click(function(){
            $('.sidebar').addClass('toggled');
            if ($(".sidebar").hasClass("toggled")) {
                $('.sidebar .collapse').collapse('hide');
                $('.mobile_overlay').removeClass('mobile_overlay_click');
                $('#content-wrapper').removeClass('screen-lock');
            };
        })
    </script>
